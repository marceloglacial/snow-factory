{
    "extends": ["next/core-web-vitals", "prettier"],
    "rules": {
        "no-unused-vars": "error",
        "no-console": [
            "error",
            {
                "allow": ["warn", "error"]
            }
        ]
    },
    "overrides": [
        {
            "files": ["*.d.ts"],
            "rules": {
                "no-unused-vars": "off"
            }
        }
    ]
}
